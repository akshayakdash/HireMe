<style>
    #footer-container {
        display: none;
    }

    #stars > i.selected {
        color: #FF912C;
    }
</style>
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script type="text/javascript">
    var jobOfferId = 0;
    var ratingValue = 0;
    $(document).ready(function () {
        $('#stars i').on('click', function () {
            var onStar = parseInt($(this).data('value'), 10); // The star currently selected
            var stars = $(this).parent().children('i');

            for (i = 0; i < stars.length; i++) {
                $(stars[i]).removeClass('selected');
            }

            for (i = 0; i < onStar; i++) {
                $(stars[i]).addClass('selected');
            }

            // JUST RESPONSE (Not needed)
            ratingValue = parseInt($('#stars i.selected').last().data('value'), 10);


        });
    })


    function setJobOfferId(jrId) {
        jobOfferId = jrId;
        $("#note-list").html("");
        // get the partial view for list of jobRequestNotes
        $.ajax
            ({
                url: '/JobOffers/GetJobOfferNotePartialView?jobOfferId=' + jobOfferId,
                //data: { "id": jobRequestId },
                type: 'get',
                success: function (result) {
                    console.log(result);
                    //$('.modal-box').text(result).fadeIn(700, function () {
                    //    setTimeout(function () {
                    //        $('.modal-box').fadeOut();
                    //    }, 2000);
                    //});

                    $("#note-list").html("");
                    $("#note-list").html(result);

                }
            });
    }

    function saveJobOfferNote() {







        var note = $("#jobOfferNote").val();
        var rating = ratingValue//$("#rating").val();
        console.log(rating);
        console.log(note);
        console.log(jobOfferId);
        if (jobOfferId && note && rating) {
            $.ajax
                ({
                    url: '/JobOffers/SaveJobOfferNote',
                    data: { "JobOfferId": jobOfferId, "Note": note, "StarRating": rating },
                    type: 'post',
                    success: function (result) {
                        $("#jobOfferNote").val("");
                        $("#rating").val("");
                        $("#modalnote1").modal("hide");
                        alert("Job Request note added successfully.");
                    }
                });
        } else {
            // give error message
            alert("Please fill up the fields")
        }
    }
</script>



<script type="text/javascript">
    function deleteFavouriteJobOffer(jobOfferId) {

        //alert(jobRequestId);
        $.ajax
            ({
                url: '/FavouriteJobOffers/Delete?id=' + jobOfferId,
                //data: { "id": jobRequestId },
                type: 'get',
                success: function (result) {
                    //$('.modal-box').text(result).fadeIn(700, function () {
                    //    setTimeout(function () {
                    //        $('.modal-box').fadeOut();
                    //    }, 2000);
                    //});

                    alert("Job Offer deleted from your favourite list.");
                    window.location.reload();
                }
            });

    }
</script>

